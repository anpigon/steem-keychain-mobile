  "author": {
    "name": "anpigon",
    "email": "markan82@gmail.com",
    "url": "https://www.steemit.com/@anpigon"
  },
  "license": "MIT",
  "version": "1.3.0",
  "private": true,
  "keywords": [
    "steem",
    "blockchain",
    "wallet",
    "cryptocurrency",
    "react native"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/anpigon/steem-keychain-mobile.git"
  },
  "engine": {
    "node": ">=12.0.0 <16.0.0"
  },
  "scripts": {
    "android": "react-native run-android",
    "android-bundle": "rm -f android/app/build/outputs/bundle/release/app-release.aab && cd android && ./gradlew bundleRelease && cd ..",
    "android-release": "react-native run-android --variant=release",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "emulator_input": "adb shell input text",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "dep-clean": "rm -rf node_modules && npm install && npx jetify",
    "clean": "watchman watch-del-all && npm run dep-clean --cache /tmp/empty-cache && npm run android-clean && npm run start -- --reset-cache",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081â€™",
    "postinstall": "patch-package && rndebugger-open",
    "prebuild-android": "npx jetify && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "beta:android": "npm run prebuild-android && cd ./android && bundle exec fastlane beta version:patch",
    "beta:ios": "cd ./ios && bundle exec fastlane beta version:patch",
    "beta": "npm run beta:android && npm run beta:ios",
    "release:android": "npm run prebuild-android && cd ./android && bundle exec fastlane release version:patch",
    "release:ios": "cd ./ios && bundle exec fastlane release version:patch",
    "release": "npm run release:android && npm run release:ios"
  },
